{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "jsx-a11y/anchor-has-content":"warn",
        "react/prop-types": "warn",
        "jsx-a11y/href-no-hash": [0]
    }
}
